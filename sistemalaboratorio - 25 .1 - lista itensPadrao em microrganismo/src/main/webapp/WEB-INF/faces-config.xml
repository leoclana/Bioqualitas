	<converter>
		<converter-id>CpfConverter</converter-id>
		<converter-class>com.util.converter.CpfConverter</converter-class>
	</converter>
	
	<converter>
		<converter-id>GrupoConverter</converter-id>
		<converter-class>com.util.converter.GrupoConverter</converter-class>
	</converter>
	
	<converter>
		<converter-id>CnpjCpfMaskConverter</converter-id>
		<converter-class>com.util.converter.CnpjCpfMaskConverter</converter-class>
	</converter>

	<converter>
		<converter-id>SimpleEntityConverter</converter-id>
		<converter-class>com.util.converter.SimpleEntityConverter</converter-class>
	</converter>

	<converter>
        <converter-id>EnumConverter</converter-id>
        <converter-class>com.util.converter.EnumConverter</converter-class>
    </converter>
    
	<converter>
        <converter-id>MicrorganismoConverter</converter-id>
        <converter-class>com.util.converter.MicrorganismoConverter</converter-class>
    </converter>

    <converter>
        <converter-id>LaudoConverter</converter-id>
        <converter-class>com.util.converter.LaudoConverter</converter-class>
    </converter>

	<lifecycle>
